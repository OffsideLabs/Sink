    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "@vueuse/motion": "^3.0.3",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "embla-carousel-vue": "^8.6.0",
    "fuse.js": "^7.1.0",
    "globe.gl": "^2.44.0",
    "intl-parse-accept-language": "^1.0.0",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.542.0",
    "mysql-bricks": "^2.0.0",
    "nanoid": "^5.1.5",
    "pluralize": "^8.0.0",
    "qr-code-styling": "^1.9.2",
    "reka-ui": "^2.5.0",
    "tw-animate-css": "^1.3.8",
    "ua-parser-js": "^2.0.4",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "virtua": "^0.42.2",
    "vue-sonner": "^2.0.8",
    "vue3-simple-icons": "^15.6.0",
    "zod": "^3.25.76"
